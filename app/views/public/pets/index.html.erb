<div class="container">
  <div class="row">
    <h4>ポメラニアンの一覧</h4>
    <div class="col-6 title-beside">
      <% if customer_signed_in? %>
        <%= link_to "登録する", new_pet_path, class: "btn btn-brown btn-sm" %>
      <% else %>
        <%= link_to "登録する", new_customer_session_path, class: "btn btn-brown btn-sm" %>
      <% end %>
    </div>
  </div>
  <div class="row">
    <%= render 'index', questions: @questions %>
  </div>
</div>